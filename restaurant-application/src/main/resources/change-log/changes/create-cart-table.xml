<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
      http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">
    <changeSet  id="7.12.2020-15.13"  author="furkan.ekim">
        <createTable tableName="cart">
            <column autoIncrement="true" name="id" type="INT(10)">
                <constraints primaryKey="true"/>
            </column>
            <column  name="create_date" type="TIMESTAMP"/>
            <column  name="piece" type="INT(50)"/>
            <column  name="total" type="INT(50)"/>
            <column  name="product_id" type="INT(50)"/>
            <column  name="table_id" type="INT(50)"/>
            <column  name="category_id" type="INT(50)"/>
            <column  name="waiter_id" type="INT(50)"/>
        </createTable>
    </changeSet>


</databaseChangeLog>